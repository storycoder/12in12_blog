<h1><%= @post.title %></h1>
<p class="date">
	Submitted <%= time_ago_in_words(@post.created_at) %> ago
</p>
<p><%= @post.body %></p>
<%= link_to 'Edit Post', edit_post_path %>
<%= link_to 'Delete Post', post_path(@post), method: :delete, data: { confirm: 'Are you sure you want to delete #{@post.title}'} %>




