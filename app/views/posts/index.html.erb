<h1>Posts</h1>

<%= link_to 'Create Post', new_post_path %>

<table>
	<thead>
		<tr>
			<th>Title</th>
			<th>Body</th>
		</tr>
	</thead>
	<tbody>
		<% @posts.each do |post|  %>
			<tr>
				<th><%= link_to post.title, post %></th>
				<th><%= post.body %></th>
			</tr>
		<% end %>
	</tbody>
</table>